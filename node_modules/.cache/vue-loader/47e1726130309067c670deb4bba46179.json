div>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: 'LandingMarket',\n    methods: {\n      handleMaket() {\n        this.$router.push({ name: 'MarketPlace' })\n        // if (window.innerWidth < 1250) {\n        //   this.$router.push({ name: 'download-app-ido' })\n        //   if (this.getMobileOperatingSystem() === 'Android') {\n        //     window.open('https://play.google.com/store/apps/details?id=vn.beedu.blockchain')\n        //   }\n        //   if (this.getMobileOperatingSystem() === 'iOS') {\n        //     window.open('https://apps.apple.com/us/app/lynkey/id1591160349')\n        //   }\n        // } else {\n        //   this.$router.push({ name: 'MarketPlace' })\n        // }\n      },\n      getMobileOperatingSystem() {\n        let userAgent = navigator.userAgent || navigator.vendor\n\n        // Windows Phone must come first because its UA also contains \"Android\"\n        if (/windows phone/i.test(userAgent)) {\n          return 'Windows Phone'\n        }\n\n        if (/android/i.test(userAgent)) {\n          return 'Android'\n        }\n\n        // iOS detection from: http://stackoverflow.com/a/9039885/177710\n        if (/iPad|iPhone|iPod/.test(userAgent)) {\n          return 'iOS'\n        }\n\n        return 'unknown'\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .marketplace {\n    // scroll-margin-top: 100px;\n    padding-top: 156px;\n  }\n  .marketplace-container {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    .mrkt-image {\n      max-width: 708px;\n      width: 100%;\n      img {\n        width: 100%;\n      }\n    }\n    .mrkt-content {\n      text-align: left;\n      max-width: 588px;\n      width: 100%;\n    }\n  }\n  .mrkt-btn {\n    width: 245px;\n    text-decoration: none;\n  }\n  p {\n    color: white;\n    opacity: 0.8;\n    margin: 36px 0;\n    font-size: 20px;\n    line-height: 30px;\n    font-weight: 400;\n  }\n  h1 {\n    color: white;\n    font-